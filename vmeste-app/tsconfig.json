{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["components/*"],
			"@/screens/*": ["screens/*"],
			"@/navigation/*": ["navigation/*"],
			"@/lib/*": ["lib/*"],
			"@/assets/*": ["assets/*"]
		},
		"resolveJsonModule": true
	}
}
